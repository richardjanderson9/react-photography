{
  "name": "react-photography",
  "version": "0.3.0",
  "private": true,
  "type": "module",
  "description": "My photography portfolio built with React, Vite, and Tailwind CSS.",
  "author": {
    "name": "Richard Anderson",
    "linkedin": "https://www.linkedin.com/in/richardjanderson9"
  },
  "license": "MIT",
  "dependencies": {
    "@cloudinary/url-gen": "^1.22.0",
    "@fingerprintjs/fingerprintjs": "^5.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.jsx",
    "lint:fix": "eslint src/**/*.jsx --fix",
    "format": "prettier --write src/**/*.{jsx,css,json,md}",
    "prepare": "husky install",
    "postinstall": "npm run prepare",
    "check": "npm run lint && npm run format && npm run test",
    "dev": "vite"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/richardjanderson9/react-photography"
  },
  "bugs": {
    "url": "https://github.com/richardjanderson9/react-photography/issues"
  },
  "homepage": "http://localhost:3000",
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.6",
    "prettier": "^3.0.0",
    "tailwindcss": "^4.1.16",
    "vite": "^7.1.12",
    "vitest": "^4.0.5"
  },
  "engines": {
    "node": ">=22.15.0",
    "npm": ">=11.4.2"
  },
  "enginesStrict": true,
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "serialize-javascript": "^6.0.2",
    "inflight": "^1.0.6"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{jsx,js,css,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "main": "src/main.jsx",
  "module": "src/main.jsx"
}
