{
  "name": "react-photography",
  "version": "0.1.7",
  "private": true,
  "type": "module",
  "description": "My photography portfolio built with React, Vite, and Tailwind CSS.",
  "author": {
    "name": "Richard Anderson",
    "linkedin": "https://www.linkedin.com/in/richardjanderson9"
  },
  "license": "MIT",
  "dependencies": {
    "@cloudinary/react": "^1.14.3",
    "@cloudinary/url-gen": "^1.21.0",
    "@fingerprintjs/fingerprintjs": "^4.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "web-vitals": "^5.0.3"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.jsx",
    "lint:fix": "eslint src/**/*.jsx --fix",
    "format": "prettier --write src/**/*.{jsx,css,json,md}",
    "prepare": "husky install",
    "dev": "vite"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/richardjanderson9/react-photography"
  },
  "bugs": {
    "url": "https://github.com/richardjanderson9/react-photography/issues"
  },
  "homepage": "http://localhost:3000",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^4.1.11",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.15.0",
    "npm": ">=11.4.2"
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "serialize-javascript": "^6.0.2",
    "inflight": "^1.0.6"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "template": true,
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "main": "src/main.jsx",
  "module": "src/main.jsx"
}
